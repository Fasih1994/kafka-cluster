{
    "name": "CDC_ORDERS",
    "config": {
        "connector.class": "io.confluent.connect.oracle.cdc.OracleCdcSourceConnector",
        "name": "CDC_ORDERS",
        "tasks.max": 3,
        "key.converter": "io.confluent.connect.avro.AvroConverter",
        "key.converter.schema.registry.url": "http://schema-registry:8081",
        "value.converter": "io.confluent.connect.avro.AvroConverter",
        "value.converter.schema.registry.url": "http://schema-registry:8081",
        "confluent.topic.bootstrap.servers": "kafka1:9092,kafka2:9092,kafka3:9092",
        "parse.key":"true",
        "oracle.server": "oracle19c",
        "oracle.port": 1521,
	    "oracle.sid":"ORCLCDB",
	    "oracle.pdb.name":"ORCLPDB1",
        "oracle.username": "C##MYUSER",
        "oracle.password": "password",
        "start.from": "snapshot",
        "table.inclusion.regex": "ORCLPDB1.FASIH.ORDERS",
        "table.exclusion.regex": "",
        "table.topic.name.template": "${databaseName}_${tableName}",
        "connection.pool.max.size": 20,
        "confluent.topic.replication.factor": 1,
        "redo.log.topic.name": "redo-log-orders",
        "numeric.mapping": "best_fit",
        "redo.log.consumer.bootstrap.servers":"kafka1:9092,kafka2:9092,kafka3:9092",
        "topic.creation.groups": "redo",
        "topic.creation.redo.include": "redo-log-orders",
        "topic.creation.redo.replication.factor": 3,
        "topic.creation.redo.partitions": 1,
        "topic.creation.redo.cleanup.policy": "delete",
        "topic.creation.redo.retention.ms": 32400000,
        "topic.creation.default.replication.factor": 3,
        "topic.creation.default.partitions": 16,
        "topic.creation.default.cleanup.policy": "compact",
        "output.username.field": "axn_performer"
    }
}
